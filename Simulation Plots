#yaxis = 'Number of Students'


tic1 = ['# Pre-Symptimatic']
tic2= ['# Asymptomatically Infected']
tic3= ['# Symptomatically Infected']
       
       
trace0 = go.Scatter(
    x = df0.loc[tic1]['Date'],
    y = df0.loc[tic1]['median']+df0.loc[tic2]['median'].values+df0.loc[tic3]['median'].values,
